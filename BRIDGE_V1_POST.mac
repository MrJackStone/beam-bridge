  BRIDGE_V1_POSTLWALL , 1 , 'EAB_NY'  , 'F. NORMAL (NY) [kN/m]'       , 'IMG16e_LWALL_NY'

  BRIDGE_V1_POSTLWALL , 2 , 'AEAB_MX' , 'MOMENTO FLETOR (MX) [kNm/m]' , 'IMG17a_LWALL_MX'
  BRIDGE_V1_POSTLWALL , 2 , 'AEAB_MY' , 'MOMENTO FLETOR (MY) [kNm/m]' , 'IMG17b_LWALL_MY'
  BRIDGE_V1_POSTLWALL , 2 , 'AEAB_Q'  , 'F. CORTANTE [kN/m]'          , 'IMG17c_LWALL_Q'  ! , 0.55
  BRIDGE_V1_POSTLWALL , 2 , 'EAB_NX'  , 'F. NORMAL (NX) [kN/m]'       , 'IMG17d_LWALL_NX'
  BRIDGE_V1_POSTLWALL , 2 , 'EAB_NY'  , 'F. NORMAL (NY) [kN/m]'       , 'IMG17e_LWALL_NY'


