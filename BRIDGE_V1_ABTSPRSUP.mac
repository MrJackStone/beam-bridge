! BRIDGE_V1_ABTSPRSUP , deckWidth , abtType , abtCS , trvCS , lwCS , rwCS , beamOpt
! BRIDGE_V1_ABTSPRSUP , arg1      , arg2    , arg3  , arg4  , arg5 , arg6 , ARG7

cm,COMPN_valid,node

*if,arg2,eq,4,then
  ! Wings
  csys,arg3
  cmsel,s,COMPA_ABT_longWalls
  nsla,r,1
  cm,COMPN_TEMP_wings,node

  ! Left wing
  csys,arg5
  cmsel,s,COMPN_valid
  cmsel,r,COMPN_TEMP_wings
  nsel,r,loc,y,0,1e6
  sprsup,FEM_ABT_springL,0,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,4
  cmsel,s,COMPN_sup
  d,all,uy,0,,,,ux

  ! Right wing
  csys,arg6
  cmsel,s,COMPN_valid
  cmsel,r,COMPN_TEMP_wings
  nsel,r,loc,y,0,1e6
  sprsup,-FEM_ABT_springL,0,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,4
  cmsel,s,COMPN_sup
  d,all,uy,0,,,,ux

  ! Transversal wall
  csys,arg4
  cmsel,s,COMPA_ABT_tranWall
  nsla,s,1
  cmsel,r,COMPN_valid
  nsel,r,loc,x,0
  sprsup,FEM_ABT_springL,0,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,6
  cmsel,s,COMPN_sup
  d,all,ux,0

*else
  ! Longitudinal walls and wings
  cmsel,s,COMPA_ABT_longWalls
  cmsel,a,COMPA_ABT_wings
  asel,r,loc,y,-arg1,0
  nsla,s,1
  cmsel,r,COMPN_valid
  sprsup,0,FEM_ABT_springL,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,5
  cmsel,s,COMPN_sup
  d,all,uy,0

  cmsel,s,COMPA_ABT_longWalls
  cmsel,a,COMPA_ABT_wings
  asel,r,loc,y,0,arg1
  nsla,s,1
  cmsel,r,COMPN_valid
  sprsup,0,-FEM_ABT_springL,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,5
  cmsel,s,COMPN_sup
  d,all,uy,0

  ! Transversal wall
  cmsel,s,COMPA_ABT_tranWall
  nsla,s,1
  cmsel,r,COMPN_valid
  sprsup,FEM_ABT_springL,0,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,6
  cmsel,s,COMPN_sup
  d,all,ux,0
*endif

! Approach slab
cmsel,s,COMPA_ABT_appSlab
nsla,s,1
csbackup
csys,arg3
nsel,u,loc,x,0,FEM_unsupportedAppSlabLength
csrestore
cmsel,r,COMPN_valid
sprsup,,,-FEM_ABT_springL,MAT_ABT_Soil,REAL_ABT_kv,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,3
cmsel,s,COMPN_sup
cmsel,s,COMPA_ABT_pileCap
nsla,s,1
cmsel,r,COMPN_valid
cm,COMPN_remoteSup,node
*get,rnc,node,,count
cmsel,s,COMPN_sup
*get,nc,node,,count
nn=0
*do,nthNode,1,nc,1
    cmsel,s,COMPN_sup
    nn=ndnext(nn)

    *if,arg2,eq,4,then
      d,nn,uz,0
    *else
      *if,rnc,gt,0,then
        cmsel,s,COMPN_remoteSup
        nsel,a,node,,nn
        rn=nnear(nn)
        safe_cp,'UZ',rn,nn
      *else
        d,nn,uz,0
      *endif
    *endif

*enddo

! Beams
*if,ARG7,eq,1,then
  cmsel,s,COMPA_ABT_beam
  lsla,s
  nsla,s,1
  cmsel,r,COMPN_valid
  sprsup,FEM_ABT_springL,0,0,MAT_ABT_Soil,REAL_ABT_kh,TYPE_LINK180,'COMPN_sup','COMPE_sup',1,6
  cmsel,s,COMPN_sup
  d,all,ux,0
*endif
